@model IEnumerable<KivikangurBicycleExam.Models.Exam>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ExamineeName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TheoryResult)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ParkingLotResult)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SlalomResult)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CircleResult)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ExamineeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TheoryResult)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParkingLotResult)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SlalomResult)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CircleResult)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
